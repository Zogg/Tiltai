from control import gate, push, pull
from control import dockersdn

def emailsink():
    crypted_mails = gate('in', network=dockersdn('crypted_mails'){'ports': [5678], 'type': PULL})

    while True:
        email = pull(crypted_mails)
        print(email)


if __name__ == '__main__':
  emailsink()
